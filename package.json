    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "dragula": "^3.7.2",
    "express": "~4.13.1",
    "mongoose": "4.6.4",
    "mongoose-simple-random": "0.3.3",
    "morgan": "~1.6.1",
    "ng2-dragula": "^1.2.1",
    "nodemon": "^1.11.0",
    "pug": "^2.0.0-alpha6",
    "pug-cli": "1.0.0-alpha6",
    "rxjs": "^5.0.0-beta.12",
    "serve-favicon": "~2.3.0",
    "zone.js": "^0.6.26"
  },
  "devDependencies": {
