{
  "name": "angular2express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "tsc -p client/ && concurrently \"tsc -p client/ -w\" \"nodemon ./bin/www\" ",
    "clean": "find client/app -name \"*.js*\" -exec rm {} \\; && rm -fr node_modules && rm -fr client/node_modules"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "morgan": "~1.6.1",
    "nodemon": "^1.11.0",
    "pug": "^2.0.0-alpha6",
    "pug-cli": "1.0.0-alpha6",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "concurrently": "^3.0.0"
  }
}
